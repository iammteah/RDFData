    rdfs:comment
    "2019-01-10 graebe: Extracted and transformed from triz-online",
    "2019-10-05 graebe: A first raw translation to Russian and English added",
    "2020-01-12 graebe: Adjustment of the German terms with (Koltze/Souchkov 2017)",
    "2020-04-16 graebe: namespace abbreviation and principle names fixed",
    "2020-07-12 graebe: od:hasRecommendation split off, Altshuller73 Ids added";
    rdfs:label "TRIZ Database - The 40 Principles" .

od:Principle
    a owl:Class ;
    rdfs:label "A TRIZ Principle"@en, "Ein TRIZ Prinzip"@de, "ТРИЗ приём"@ru .

odq:Segmentation
  od:hasRecommendation odq:Segmentation_1, odq:Segmentation_2, odq:Segmentation_3 ; 
    od:hasAltshuller73Id "01" ;
    od:hasAltshuller84Id "01" ;
    a od:Principle ;
    rdfs:label "Principle of decomposition or segmentation"@en, "Prinzip der Zerlegung bzw. Segmentierung"@de, "Приём дробления"@ru .

odq:Segmentation_1 a od:Recommendation;  
    od:description "Zerlege das Objekt in unabhängige, gleiche Teile."@de, "Divide the object into independent, equal parts."@en, "Объект должен быть разделен на независимые равные части."@ru .

odq:Segmentation_2 a od:Recommendation; 
    od:description "Führe das Objekt zerlegbar aus."@de, "Make the object to be dismantled."@en, "Объект должен быть разложен."@ru .

odq:Segmentation_3 a od:Recommendation; 
    od:description "Erhöhe den Grad der Zerlegung des Objektes."@de, "Increase the degree of decomposition of the object."@en, "Степень разложения объекта должна быть увеличена."@ru .

odq:Separation 
  od:hasRecommendation odq:Separation_1, odq:Separation_2 ; 
    od:hasAltshuller73Id "02" ;
    od:hasAltshuller84Id "02" ;
    a od:Principle ;
    rdfs:label "Principle of separation"@en, "Gesonderte Behandlung"@de, "Prinzip der Abtrennung"@de, "Приём вынесения"@ru .

odq:Separation_1 a od:Recommendation; 
    od:description "Remove the disturbing part or property from the object."@en, "Trenne vom Objekt das störende Teil, die störende Eigenschaft, ab."@de, "Из объекта отделить мешающую часть с вредным свойством."@ru .
    
odq:Separation_2 a od:Recommendation; 
    od:description "Zerlege das Objekt in unterschiedliche Teile."@de, "Divide the object into different parts."@en, "Предлагается разделить объект на разные части."@ru .
    
odq:LocalQuality 
  od:hasRecommendation odq:LocalQuality_1, odq:LocalQuality_2 ; 
    od:hasAltshuller73Id "03" ;
    od:hasAltshuller84Id "03" ;
    a od:Principle ;
    rdfs:label "Principle of Local Quality"@en, "Schaffen optimaler Bedingungen"@de, "Prinzip der örtlichen Qualität"@de, "Приём местного качества"@ru .

odq:LocalQuality_1 a od:Recommendation; 
    od:description "Every part of the object should be under conditions that are most conducive to its work."@en, "Für jedes Teil des Objektes sind solche Bedingungen zu sichern, die seiner Arbeit am zuträglichsten sind."@de, "Каждая часть объекта должна находиться в условиях, наиболее благоприятных для его работы."@ru .
    
odq:LocalQuality_2 a od:Recommendation; 
    od:description "Transform the homogeneous structure of the object or the surrounding medium into an inhomogeneous structure."@en, "Gehe von der homogenen Struktur des Objektes oder des umgebenden Mediums zu einer inhomogenen Struktur über."@de, "Однородная структура объекта или окружающей среды должна быть преобразована в неоднородную."@ru .
    
odq:Asymmetry 
  od:hasRecommendation odq:Asymmetry_1, odq:Asymmetry_2 ; 
    od:hasAltshuller73Id "04" ;
    od:hasAltshuller84Id "04" ;
    a od:Principle ;
    rdfs:label "Principle of asymmetry"@en, "Prinzip der Asymmetrie"@de, "Приём асимметрии"@ru .

odq:Asymmetry_2 a od:Recommendation; 
    od:description "Erhöhe den Grad der Asymmetrie, wenn das Objekt bereits asymmetrisch ist."@de, "Increase the degree of asymmetry if the object is already asymmetric."@en, "Степень асимметрии должна быть увеличена, если объект уже асимметричен."@ru .
    
odq:Asymmetry_1 a od:Recommendation; 
    od:description "Move from a symmetrical shape of the object to an asymmetric one."@en, "Gehe von einer symmetrischen Form des Objektes zu einer asymmetrischen über."@de, "От симметричной формы объекта необходимо перейти к асимметричной."@ru . 
    
odq:Coupling 
  od:hasRecommendation odq:Coupling_1 ; 
    od:hasAltshuller73Id "05" ;
    od:hasAltshuller84Id "05" ;
    a od:Principle ;
    rdfs:label "Principle of coupling"@en, "Kombination"@de, "Prinzip der Kopplung"@de, "Приём объединения"@ru .

odq:Coupling_1 a od:Recommendation; 
    od:description "Kopple gleichartige oder zu koordinierende Systeme oder Operationen."@de, "Couple similar or coordinating systems or operations."@en, "Подобные или координирующие системы или операции должны быть объединены."@ru .
    
odq:Universality 
  od:hasRecommendation odq:Universality_1 ; 
    od:hasAltshuller73Id "06" ;
    od:hasAltshuller84Id "06" ;
    a od:Principle ;
    rdfs:label "Principle of universality"@en, "Mehrzwecknutzung"@de,"Prinzip der Universalität"@de,  "Приём универсальности"@ru .

odq:Universality_1 a od:Recommendation; 
    od:description "Das Objekt erfüllt mehrere unterschiedliche Funktionen, wodurch weitere gesonderte Objekte überflüssig werden."@de, "The object fulfills several different functions, which makes additional separate objects superfluous."@en, "Объект выполняет несколько различных функций, что делает ненужными дополнительные объекты."@ru .
    
odq:Nesting 
  od:hasRecommendation odq:Nesting_1, odq:Nesting_2 ;
    od:hasAltshuller73Id "07" ;
    od:hasAltshuller84Id "07" ;
    a od:Principle ;
    rdfs:label "Principle of nesting (matryoshka)"@en, "Prinzip der Integration (Matrjoschka-Prinzip)"@de, "Приём вложенности («матрёшки»)"@ru .

odq:Nesting_1 a od:Recommendation; 
    od:description "Place an object inside another, which in turn is inside a third."@en, "Bringe ein Objekt im Inneren eines anderen unter, das sich wiederum im Inneren eines dritten befindet."@de, "Объект размещается внутри другого, который, в свою очередь, находится внутри третьего."@ru .
    
odq:Nesting_2 a od:Recommendation; 
    od:description "An object passes through or fills the cavity of another object."@en, "Ein Objekt durchläuft oder füllt den Hohlraum eines anderen Objektes."@de, "Объект проходит через или заполняет полость другого объекта."@ru . 
    
odq:Countermass 
  od:hasRecommendation odq:Countermass_1, odq:Countermass_2 ;
  od:hasAltshuller73Id "08" ;
    od:hasAltshuller84Id "08" ;
    a od:Principle ;
    rdfs:label "Principle of the countermass"@en, "Prinzip des Gegengewichts"@de, "Приём противовеса"@ru .

odq:Countermass_1 a od:Recommendation; 
    od:description "Kompensiere das Gewicht des Objektes durch die Kopplung mit einem anderen Objekt, das Tragkraft besitzt."@de, "Compensate the weight of the object by coupling it with another object that has bearing capacity."@en, "Вес объекта должен быть компенсирован путем соединения его с другим объектом, который обладает несущей способностью."@ru  .
    
odq:Countermass_2 a od:Recommendation; 
    od:description "Kompensiere das Gewicht des Objektes durch Wechselwirkung mit einem Medium."@de, "Compensate the weight of the object by interaction with a medium."@en, "Вес объекта должен быть компенсирован взаимодействием со средой."@ru .
    
odq:EarlyCounteraction 
  od:hasRecommendation odq:EarlyCounteraction_1 ;
    od:hasAltshuller73Id "09" ;
    od:hasAltshuller84Id "09" ;
    a od:Principle ;
    rdfs:label "Principle of early counteraction"@en, "Prinzip der Vorspannung"@de, "Prinzip der vorherigen Gegenwirkung"@de, "Приём предварительного противодействия"@ru .

odq:EarlyCounteraction_1 a od:Recommendation; 
    od:description "If a certain effect is to be achieved according to the conditions of the task, first generate the counteraction."@en, "Wenn gemäß den Bedingungen der Aufgabe eine bestimmte Wirkung erzielt werden soll, erzeuge vorher die Gegenwirkung."@de, "Чтобы в соответствии с условиями задачи достигать определенный эффект, сначала должно быть создано противодействие."@ru .
    
odq:EarlyCounteraction_2 a od:Recommendation; 
    od:description "Erzeuge im Objekt vorab eine Gegenwirkung, die der unzulässigen oder unerwünschten Wirkung entgegengesetzt ist (Prinzip der Vorspannung)."@de, "Заранее придать объекту напряжения, противоположные недопустимым или нежелательным рабочим напряжениям."@ru .
    
odq:PreferredAction 
  od:hasRecommendation odq:PreferredAction_1, odq:PreferredAction_2 ;
    od:hasAltshuller73Id "10" ;
    od:hasAltshuller84Id "10" ;
    a od:Principle ;
    rdfs:label "Principle of the preferred action"@en, "Vorfertigung"@de, "Prinzip der vorherigen Wirkung"@de, "Приём предварительного действия (заранее выполненное действие)"@ru .

odq:PreferredAction_1 a od:Recommendation; 
    od:description "Bereite die Objekte vorab so vor, dass sie ohne Zeitverlust vom geeignetsten Ort aus wirken können."@de, "Prepare the objects in advance in such a way that they can work without delay at the most suitable place."@en, "Объекты сначала должны быть настроены таким образом, чтобы они могли работать без задержки из наиболее подходящего места."@ru .
    
odq:PreferredAction_2 a od:Recommendation; 
    od:description "Erziele die erforderliche Wirkung vorab (vollständig oder auch teilweise)."@de, "Achieve the required effect beforehand (completely or partially)."@en, "Требуемый эффект должен быть достигнут заранее (полностью или частично)."@ru .
    
odq:Prevention 
  od:hasRecommendation odq:Prevention_1, odq:Prevention_2 ;
    od:hasAltshuller73Id "11" ;
    od:hasAltshuller84Id "11" ;
    a od:Principle ;
    rdfs:label "Principle of advanced cushion (prevention)"@en, "Vorbeugen"@de, "Prinzip der Prävention"@de, "Приём заранее подложенной подушки"@ru .

odq:Prevention_1 a od:Recommendation; 
    od:description "Compensate the relatively low reliability of the object by means previously provided."@en, "Kompensiere die relativ geringe Zuverlässigkeit des Objektes durch vorher bereitgestellte Hilfsmittel."@de, "Относительно низкая надежность объекта компенсируется ранее предоставленными средствами."@ru .
    
odq:Prevention_2 a od:Recommendation; 
    od:description "Заранее предусматривается устройство для выполнения спасательных и других аваририйных действий."@ru, "Sieh eine Einrichtung zur Durchführung von Rettungs- und anderen Notfallmaßnahmen vor."@de, "A device is foreseen for performing rescue and other emergency actions."@en .
    
odq:Equipotentiality 
  od:hasRecommendation odq:Equipotentiality_1 ;
    od:hasAltshuller73Id "12" ;
    od:hasAltshuller84Id "12" ;
    a od:Principle ;
    rdfs:label "Principle of Equipotentiality"@en, "Äquipotentieller Transport"@de, "Prinzip der Äquipotenzialität"@de, "Приём эквипотенциальности"@ru .

odq:Equipotentiality_1 a od:Recommendation; 
    od:description "Verändere die Arbeitsbedingungen so, dass das Objekt mit konstantem Energiepotenzial arbeiten kann, z.B. nicht angehoben oder herabgelassen werden muss."@de, "Change the working conditions in such a way that the object can work with a constant energy potential, and has not to be raised or lowered."@en, "Рабочие условия должны быть изменены таким образом, чтобы объект мог работать с постоянным энергетическим потенциалом, его не нужно поднимать или опускать."@ru .
    
odq:Conversely 
  od:hasRecommendation odq:Conversely_1, odq:Conversely_2, odq:Conversely_3 ;
    od:hasAltshuller73Id "13" ;
    od:hasAltshuller84Id "13" ;
    a od:Principle ;
    rdfs:label "Prinzip der Funktionsumkehr (Inversion)"@de, "Principle conversely"@en, "Приём Наоборот"@ru .

odq:Conversely_1 a od:Recommendation; 
    od:description "Instead of the effect prescribed by the conditions of the task, achieve the reverse effect."@en, "Erziele statt der Wirkung, die durch die Bedingungen der Aufgabe vorgeschrieben wird, die umgekehrte Wirkung."@de, "Вместо эффекта, предписанного условиями задачи, достигается обратный эффект."@ru .
    
odq:Conversely_2 a od:Recommendation; 
    od:description "Stelle das Objekt auf den Kopf bzw. kehre es um."@de, "Put the object upside down or reverse it."@en, "Перевернуть объект вверх ногами или другим способом наоборот."@ru  .
    
odq:Conversely_3 a od:Recommendation; 
    od:description "Gestalte den beweglichen Teil des Objektes oder des umgebenden Mediums unbeweglich, und den unbeweglichen beweglich."@de, "Make the moving part of the object or the surrounding medium immobile, and the immovable movable."@en, "Сделай движущуюся часть объекта или окружающую среду неподвижной, а неподвижную - подвижной."@ru  .
    
odq:BallSimilarity 
  od:hasRecommendation odq:BallSimilarity_1, odq:BallSimilarity_2, odq:BallSimilarity_3 ;
    od:hasAltshuller73Id "14" ;
    od:hasAltshuller84Id "14" ;
    a od:Principle ;
    rdfs:label "Principle of the ball similarity"@en, "Prinzip der sphärischen Form"@de, "Приём сфероидальности"@ru .

odq:BallSimilarity_1 a od:Recommendation;
    od:description "From straight contours go to curved ones, from flat surfaces go to spherical ones."@en, "Gehe von geradlinigen Konturen zu gekrümmten, von ebenen Flächen zu sphärischen über."@de, "От прямых контуров переходить к изогнутым, от плоских поверхностей к сферическим."@ru .
    
odq:BallSimilarity_2 a od:Recommendation;
    od:description "Move from the rectilinear motion to rotation."@en, "Gehe von einer geradlinigen Bewegung zur Rotation über."@de, "От прямолинейного движения переходить к вращению."@ru .
    
odq:BallSimilarity_3 a od:Recommendation;
    od:description "Use rollers, balls, spirals."@en, "Verwende Rollen, Kugeln, Spiralen."@de, "Используются ролики, шарики, спирали."@ru .
    
odq:Dynamization 
  od:hasRecommendation odq:Dynamization_1, odq:Dynamization_2, odq:Dynamization_3 ; 
    od:hasAltshuller73Id "15" ;
    od:hasAltshuller84Id "15" ;
    a od:Principle ;
    rdfs:label "Principle of dynamization"@en, "Prinzip der ständigen Anpassung"@de, "Prinzip der Dynamisierung"@de, "Приём динамичности"@ru .

odq:Dynamization_1 a od:Recommendation;
    od:description "If the object as a whole is immobile, make it movable (adjustable)."@en, "Если объект в целом неподвижен, он должен стать подвижным (регулируемым)."@ru, "Falls das Objekt insgesamt unbeweglich ist, gestalte es beweglich (verstellbar)."@de .
    
odq:Dynamization_2 a od:Recommendation;
    od:description "Zerlege das Objekt in Teile, die sich zueinander verstellen oder verschieben lassen."@de, "Brake down the object into parts that can be displaced or shifted."@en, "Объект разбить на части, которые могут быть смещены или сдвинуты."@ru .
    
odq:Dynamization_3 a od:Recommendation;
    od:description "Verändere die Kennwerte des Objektes (oder des umgebenden Mediums) so, dass sie in jeder Arbeitsetappe optimal sind (Anpassung)."@de, "Change the characteristics of the object (or the surrounding medium) to be optimal in each work step (adaptation)."@en, "Характеристики объекта (или окружающей среды) должны меняться, чтобы быть оптимальными на каждом этапе работы (адаптация)."@ru .
    
odq:PartialOrExcessEffect 
  od:hasRecommendation odq:PartialOrExcessEffect_1 ; 
    od:hasAltshuller73Id "16" ;
    od:hasAltshuller84Id "16" ;
    a od:Principle ;
    rdfs:label "Principle of partial or excess effect"@en, "Prinzip der nicht vollständigen Lösung"@de, "Prinzip der partiellen oder überschüssigen Wirkung"@de, "Приём частичного или избыточного действия"@ru .

odq:PartialOrExcessEffect_1 a od:Recommendation;
    od:description "If 100% of the required effect is difficult to achieve, then achieve a little less or a little bit more."@en, "Wenn 100% des erforderlichen Effekts schwer zu erzielen sind, erziele „ein bisschen weniger“ oder „ein bisschen mehr“."@de, "Если 100% требуемого эффекта трудно достичь, то достичь немного меньше или немного больше."@ru .
    
odq:TransitionToAnotherDimension 
  od:hasRecommendation odq:TransitionToAnotherDimension_1, odq:TransitionToAnotherDimension_2, odq:TransitionToAnotherDimension_3, odq:TransitionToAnotherDimension_4 ;
    od:hasAltshuller84Id "17" ;
    a od:Principle ;
    rdfs:label "Principle of transition to another dimension"@en, "Prinzip des Übergangs in eine andere Dimension"@de, "Приём перехода в другое измерение"@ru .

odq:TransitionToAnotherDimension_1 a od:Recommendation;
    od:description "Instead of arrangement in only a layer arrange objects in multiple layers. "@en, "Вместо размещения только в один слой размещать объекты в нескольких слоях."@ru, "Ordne die Objekte statt in nur einer Ebene in mehreren Ebenen angeordnet."@de .
    
odq:TransitionToAnotherDimension_2 a od:Recommendation;
    od:description "Stelle das Objekt geneigt auf."@de, "The object is inclined to be placed."@en, "Объект размещать склонным образом."@ru .

odq:TransitionToAnotherDimension_3 a od:Recommendation;
    od:description "Nutze die Rückseite des gegebenen Objektes aus."@de, "Exploit the back of the given object."@en, "Использовать обратную сторону данного объекта."@ru .

odq:TransitionToAnotherDimension_4 a od:Recommendation;
    od:description "Difficulties from object's motion on a line are eliminated when the object moves in two dimensions. Difficulties associated with the movement of objects on a plane are eliminated by the transition to three-dimensional space."@en, "Bei Schwierigkeiten der Bewegung eines Objektes längs einer Linie gehe zu zwei Dimensionen über, d.h. bewege es in einer Ebene. Analog werden Schwierigkeiten einer zweidimensionalen Bewegung von Objekten durch Übergang zum dreidimensionalen Raum beseitigt."@de, "Трудности, возникающие в результате привязки движения объекта к линии, устраняются, когда объект перемещается в двух измерениях, то есть в плоскости. Трудности, связанные с перемещением объектов на плоскости устраняются при переходе в трехмерное пространство."@ru .

odq:ExploitationOfMechanicalVibrations 
  od:hasRecommendation odq:ExploitationOfMechanicalVibrations_1, odq:ExploitationOfMechanicalVibrations_2, odq:ExploitationOfMechanicalVibrations_3, odq:ExploitationOfMechanicalVibrations_4, odq:ExploitationOfMechanicalVibrations_5 ; 
    od:hasAltshuller84Id "18" ;
    a od:Principle ;
    rdfs:label "Principle of exploitation of mechanical vibrations"@en, "Prinzip der Ausnutzung mechanischer Schwingungen"@de, "Приём использования механических колебаний"@ru .

odq:ExploitationOfMechanicalVibrations_1 a od:Recommendation;
    od:description "Verwende Piezovibratoren statt mechanischer Vibratoren."@de, "Use piezovibrators instead of mechanical ones."@en, "Вместо механических вибраторов должны использоваться пьезовибраторы."@ru .

odq:ExploitationOfMechanicalVibrations_2 a od:Recommendation;
    od:description "Nutze Ultraschallschwingungen in Verbindung mit elektromagnetischen Feldern."@de, "Use ultrasonic vibrations in conjunction with electromagnetic fields."@en, "Используйте ультразвуковые колебания в сочетании с электромагнитными полями."@ru .

odq:ExploitationOfMechanicalVibrations_3 a od:Recommendation;
    od:description "Versetze das Objekt in Schwingungen."@de, "Vibrate the object."@en, "Объект должен вибрировать."@ru  .

odq:ExploitationOfMechanicalVibrations_4 a od:Recommendation;
    od:description "Nutze die Eigenfrequenz aus."@de, "Exploit the eigen frequency."@en, "Собственная частота должна эксплуатироваться."@ru  .

odq:ExploitationOfMechanicalVibrations_5 a od:Recommendation;
    od:description "Falls eine solche Bewegung bereits vorliegt, erhöhe ihre Frequenz bis hin zur Ultaschallfrequenz."@de, "If such a movement already exists, increase its frequency, up to the ultrasonic frequency."@en, "Если такое движение уже существует, его частота должна быть увеличена до ультразвуковой."@ru .

odq:PeriodicEffect 
  od:hasRecommendation odq:PeriodicEffect_1, odq:PeriodicEffect_2, odq:PeriodicEffect_3 ;
    od:hasAltshuller73Id "19" ;
    od:hasAltshuller84Id "19" ;
    a od:Principle ;
    rdfs:label "Prinzip der Impuls-Arbeitsweise"@de, "Prinzip der periodischen Wirkung"@de, "Principle of periodic effect"@en, "Приём периодического действия"@ru .

odq:PeriodicEffect_1 a od:Recommendation;
    od:description "Nutze die Pausen zwischen den Impulsen für eine andere Wirkung aus."@de, "Exploit the pauses between the pulses for another effect."@en, "Паузы между импульсами должны использоваться для другого эффекта."@ru .

odq:PeriodicEffect_2 a od:Recommendation;
