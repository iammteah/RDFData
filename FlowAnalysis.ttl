tc:FlowComposition
    a skos:Concept, od:AdditionalConcept ;
    rdfs:subClassOf tc:Flow ;
    rdf:type tc:InformationFlow, tc:EnergyFlow, tc:SubstanceFlow ;
    skos:prefLabel "Composition of flow"@en, "Flusskomposition"@de ;
    skos:altLabel "Zusammensetzung des Flusses"@de .

tc:FlowFunctionality
    a skos:Concept, od:AdditionalConcept ;
    rdfs:subClassOf tc:Flow ;
    rdf:type tc:usefulFlow, tc:usefulFlowWith tc:harmfulFlow, tc:parasiticFlow ;
    skos:prefLabel "Functionality of the flow"@en, "Funktionalität des Flusses"@de .

tc:FlowSource
    a skos:Concept, od:AdditionalConcept ;
    rdfs:subClassOf tc:Flow ;
    rdf:type tc:primaryFlow, tc:secondaryFlow ;
    skos:prefLabel "Source of flow"@en, "Flussquelle"@de .


tc:StaticFlowComponent
    a skos:Concept, od:AdditionalConcept ;
    od:isPartOf tc:FlowModel ;
    od:LebedevDefinition "forming the stream and specify its initial parameters" ;
    od:includes tc:ControlUnit, tc:Receiver, tc:Source, tc:Channel ;
    skos:prefLabel "static components of the flow"@en, "statische Flusskomponenten"@de .

tc:ControlUnit
    skos:prefLabel "ControlUnit"@en ;
    rdfs:subClassOf tc:StaticFlowComponent ;
    rdf:type tc:Pump, tc:Valve ;
    skos:prefLabel "Control Unit"@en, "Steuerungssystem"@de ;
    skos:altLabel "Management System"@en, "Managementsystem"@de .

tc:Receiver
    a skos:Concept, od:AdditionalConcept ;
    od:LebedevTerm "Product" ;
    od:LebedevDefinition "That component of the system that is transformed under the influence of a given useful flow(changing at least one of its parameters), or which is directly damaged by the considered harmful flow" ;
    rdfs:subClassOf tc:StaticFlowComponent ;
    skos:prefLabel "Receiver"@en, "Empfänger"@de ;
    skos:altLabel "Product"@en .

tc:Source
    a skos:Concept, od:AdditionalConcept ;
    rdfs:subClassOf tc:StaticFlowComponent ;
    rdf:type tc:EnergySource, tc:SourceOfPotential ;
    skos:prefLabel "Source"@en, "Quelle"@de .

tc:Channel
    a skos:Concept, od:AdditionalConcept ;
    od:LebedevDefinition "The component in which the flow moves directly through the system." ;
    rdfs:subClassOf tc:StaticFlowComponent ;
    skos:prefLabel "Channel"@en, "Kanal"@de .


tc:MethodOfDescription
    a skos:Concept, od:AdditionalConcept ;
    od:isPartOf tc:FlowModel ;
    od:includes tc:TextDescription, tc:ParametricDescription, tc:GraphicDescription ;
    skos:prefLabel "Method of description"@en, "Methode zur Beschreibung"@de .


tc:TextDescription
    a skos:Concept, od:AdditionalConcept ;
    rdfs:subClassOf tc:MethodOfDescription ;
    skos:prefLabel "textual description"@en, "Beschreibungstext"@de .

tc:ParametricDescription
    a skos:Concept, od:AdditionalConcept ;
    rdfs:subClassOf tc:MethodOfDescription ;
    skos:prefLabel "parametric description"@en, "parametrische Beschreibung"@de .

tc:GraphicDescription
    a skos:Concept, od:AdditionalConcept ;
    rdfs:subClassOf tc:MethodOfDescription ;
    skos:prefLabel "graphical description"@en, "graphische Beschreibung"@de .

# Top Level Flow Analysis Model
tc:FlowAnalysis
    cc:attributionName "Flow Analysis Model" ;
    a owl:Ontology ;
    a rdfs:class, od:SouchkovGlossaryEntry, skos:Concept;
    od:includes tc:Flow, tc:FlowModel, tc:MethodOfDescription ;
    rdfs:comment "prescriptive base of the flow analysis model" ;
    rdfs:label "Flow Analysis Model of Olga Eckardt" ;
    skos:prefLabel "Flow Analysis Model"@en, "Modell der Flussanalyse"@de .

tc:FlowModel
    od:includes tc:ActualFlowModel, tc:TargetFlowModel .

tc:ActualFlowModel
    od:includes tc:RulesOfEvaluation, tc:RulesOfConstruction ;
    od:relatedTo tc:TechniquesOfModification ;
    od:isPartOf tc:FlowModel .

tc:TargetFlowModel
    od:includes tc:RulesOfConstructionOfInConformRequirements, tc:RulesOfApplyingLawsOfDevelopmentOfTechnicalSystems ;
    od:relatedTo tc:TechniquesOfModification ;
    od:isPartOf tc:FlowModel .

tc:TechniquesOfModification
    od:issuedAt tc:ActualFlowModel ;
    od:forms tc:TargetFlowModel .


tc:RulesOfEvaluation
    od:issuedAt tc:ActualFlowModel ;
    od:isPartOf tc:RulesOfFlowAnalysis .

tc:RulesOfConstruction
    od:forms tc:ActualFlowModel ;
    od:isPartOf tc:RulesOfFlowAnalysis .

tc:RulesOfConstructionOfInConformRequirements
    od:isPartOf tc:RulesOfFlowAnalysis, tc:TargetFlowModel .
# Problemmodell??

tc:RulesOfApplyingLawsOfDevelopmentOfTechnicalSystems
    od:isPartOf tc:RulesOfFlowAnalysis, tc:TargetFlowModel .

tc:RulesOfFlowAnalysis
    od:isPartOf tc:FlowAnalysis ;
    od:includes tc:Flow, tc:Goal, tc:RulesOfFlowAnalysis .

tc:Goal
    od:isPartOf tc:FlowAnalysis ;
    od:isDefinedBy tc:SetupContext ;
    od:defines tc:ProofRequirements .

tc:SetupContext
    od:IsDefinedBy tc:Goal ;
    od:defines tc:Goal .

tc:ProofRequirements
    od:isPartOf tc:Goal ;
    od:leadsTo tc:IncreaseUsefulFlow, tc:ReduceHarmfulFlow, tc:ReduceParasiticFlow .

tc:IncreaseUsefulFlow
    od:basedOn tc:ProofRequirements .

tc:ReduceHarmfulFlow
    od:basedOn tc:ProofRequirements ;
    od:includes tc:ReduceParasiticFlow .

tc:ReduceParasiticFlow
    od:isPartOf tc:ReduceHarmfulFlow ;
    od:basedOn tc:ProofRequirements .